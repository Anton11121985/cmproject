<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.2.xsd"
       default-lazy-init="true">

    <bean id="PersonDirectory_(vw_cmj_employees_by_name_vcard)Metadata"
          class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.CollectionMetadataNew" p:searchArea="so_personnonsysorg_and_private_search_uicoll">
        <constructor-arg>
            <list value-type="ru.intertrust.cm_sochi.srv.connector.sochi.collections.Field">
                <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.Field" p:name="icon">
                    <property name="virtualField">
                        <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.IconVirtualField" p:name="icon">
                            <property name="realFields">
                                <list>
                                    <value>iconId</value>
                                    <value>iconHint</value>
                                </list>
                            </property>
                            <property name="separator" value=""/>
                            <property name="emptySeparator" value="true"/>
                        </bean>
                    </property>
                </bean>
                <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.Field" p:name="origShortName" p:filter="origShortName" p:sortOrder="ASCENDING">
                    <property name="virtualField">
                        <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.BuildVirtualField" p:name="origShortName">
                            <property name="realFields">
                                <list>
                                    <value>origShortName</value>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
                <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.Field" p:name="shortNameOrganization" p:filter="shortNameOrganization">
                    <property name="virtualField">
                        <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.BuildVirtualField" p:name="shortNameOrganization">
                            <property name="realFields">
                                <list>
                                    <value>shortNameOrganization</value>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
                <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.Field" p:name="region" p:filter="region">
                    <property name="virtualField">
                        <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.BuildVirtualField" p:name="region">
                            <property name="realFields">
                                <list>
                                    <value>region</value>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
                
                <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.Field" p:name="depShortName"
                      p:filter="depShortName_partial">
                    <property name="virtualField">
                        <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.BuildVirtualField" p:name="depShortName">
                            <property name="realFields">
                                <list>
                                    <value>depShortName</value>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
                
                <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.Field" p:name="soPost" p:filter="soPost">
                    <property name="virtualField">
                        <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.BuildVirtualField" p:name="soPost">
                            <property name="realFields">
                                <list>
                                    <value>soPost</value>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
                <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.Field" p:name="soAddress" p:filter="soAddress">
                    <property name="virtualField">
                        <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.BuildVirtualField" p:name="soAddress">
                            <property name="realFields">
                                <list>
                                    <value>soAddress</value>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
                <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.Field" p:name="origPhone" p:filter="origPhone">
                    <property name="virtualField">
                        <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.BuildVirtualField" p:name="origPhone">
                            <property name="realFields">
                                <list>
                                    <value>origPhone</value>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
                <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.Field" p:name="vcardEmail" p:filter="vcardEmail_partial">
                    <property name="virtualField">
                        <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.BuildVirtualField" p:name="vcardEmail">
                            <property name="realFields">
                                <list>
                                    <value>vcardEmail</value>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
                <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.Field" p:name="Form" p:filter="Form">
                    <property name="virtualField">
                        <bean class="ru.intertrust.cm_sochi.srv.connector.sochi.collections.BuildVirtualField" p:name="Form">
                            <property name="realFields">
                                <list>
                                    <value>form</value>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
            </list>
        </constructor-arg>
    </bean>
</beans>