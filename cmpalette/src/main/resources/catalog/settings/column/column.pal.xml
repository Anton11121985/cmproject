<?xml version='1.0' encoding='UTF-8'?>
<catalog xmlns="http://www.intertrust.ru/schema/palette/cat-ui">
  <column name="wfProcessId" extends="string" subject="${catalog.column.wfProcessId.subject:ИД-процесса}"/>
  <column name="wfVersion" extends="string" subject="${catalog.column.wfVersion.subject:Версия}"/>
  <complex-column name="wfAssignee" subject="${catalog.column.wfAssignee.subject:Исполнители}" extends="vcard">
    <column-ref ref="shortName"/>
    <column-ref ref="id"/>
    <column-ref ref="hint"/>
  </complex-column>
  <column name="wfStage" extends="string" subject="${catalog.column.wfStage.subject:Стадия}"/>
  <column name="wfStatus" extends="string" subject="${catalog.column.wfStatus.subject:Статус}"/>
  <column name="wfProcessName" extends="string" subject="${catalog.column.wfProcessName.subject:Схема}"/>
  <column name="wfTerm" extends="date" subject="${catalog.column.wfTerm.subject:Срок}"/>
  <column name="wfSubject" extends="string" subject="${catalog.column.wfSubject.subject:Заголовок}"/>
  <column name="wfTask" extends="string" subject="${catalog.column.wfTask.subject:Задача}"/>
  <column name="wfNumber" extends="string" subject="${catalog.column.wfNumber.subject:Номер}"/>
  <column name="wfStart" extends="date" subject="${catalog.column.wfStart.subject:Дата начала стадии}"/>

  <column name="relation_name" extends="string" subject="${catalog.column.relation_name.subject:Название}"/>
  <column name="relation_description" extends="string" subject="${catalog.column.relation_description.subject:Описание}"/>
  <column name="relation_formula" extends="string" subject="${catalog.column.relation_formula.subject:Формула}"/>
</catalog>