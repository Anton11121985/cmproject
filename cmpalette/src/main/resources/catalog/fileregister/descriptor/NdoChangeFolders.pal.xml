    <custom-descriptor ident="FileRegister" view="(vw_cmj_ndochange_trash)" id="fr-des-vw-cmj-ndochange-trash" name="Корзина"
                       style="default">
        <help id="0"/>
        <builder name="ru.intertrust.cm_sochi.srv.uicollections.UiBuilder"/>
