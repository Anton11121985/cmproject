<?xml version='1.0' encoding='UTF-8'?>
<catalog xmlns="http://www.intertrust.ru/schema/palette/cat-ui">
  <custom-descriptor ident="FileRegister" view="(include_ndp_change)" id="include-ndp-change" name="Дочерние ИкНДП" style="default">
    <builder name="ru.intertrust.cmj.af.collections.impl.BuilderDominoView">
      <config>
        <view-complex-classif hide-checkboxes="true"/>
      </config>
    </builder>
    <table>
      <columns>
        <column-ref ref="date"/>
        <column-ref ref="changeNumber"/>
        <column-ref ref="statusGeneral"/>
        <column-ref ref="currentNdp" category="true"/>
        <column-ref ref="self" />
      </columns>
    </table>
  </custom-descriptor>

</catalog>