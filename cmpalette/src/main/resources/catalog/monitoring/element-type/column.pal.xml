<?xml version='1.0' encoding='UTF-8'?>
<catalog xmlns="http://www.intertrust.ru/schema/palette/cat-ui">
    <column name="noticeStatusText" subject="${catalog.column.noticeStatusText.subject:Статус}" extends="string">
        <meta>
            <documentation>Статус</documentation>
        </meta>
    </column>
    <column name="read" subject="${catalog.column.read.subject:Прочитано}" extends="datetime">
        <meta>
            <documentation>Прочитано</documentation>
        </meta>
    </column>
    <column name="refreshed" subject="${catalog.column.refreshed.subject:Обновлено}" extends="datetime">
        <meta>
            <documentation>Обновлено</documentation>
        </meta>
    </column>
    <complex-column name="recipient" subject="${catalog.column.recipient.subject:Получатель}" extends="vcard">
        <meta>
            <documentation>Получатель</documentation>
        </meta>
        <column-ref ref="shortName"/>
        <column-ref ref="id"/>
        <column-ref ref="hint"/>
    </complex-column>
    <column name="taskStatusText" subject="${catalog.column.taskStatusText.subject:Статус}" extends="string">
        <meta>
            <documentation>Статус</documentation>
        </meta>
    </column>
    <column name="eventType" subject="${catalog.column.eventType.subject:Тип}" extends="string">
        <meta>
            <documentation>Тип</documentation>
        </meta>
    </column>
</catalog>