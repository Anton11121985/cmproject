<?xml version='1.0' encoding='UTF-8'?>
<catalog xmlns="http://www.intertrust.ru/schema/palette/cat-ui">
  <custom-descriptor ident="InputDocs" view="InputDocs_(catalog_template_input_doc)" id="InputDocs-Catalog-Template-Input-Doc"
                     name="ВхД - Справочники/Шаблоны/Входящих/документов" style="default">
    <builder name="ru.intertrust.cm_sochi.srv.uicollections.UiBuilder"/>
    <table>
      <columns>
        <column-ref ref="self"/>
        <column-ref ref="templateName" filter="true"/>
        <column-ref ref="accessTypeColl" filter="true"/>
        <column-ref ref="subject" filter="true"/>
        <column-ref ref="reqType" filter="true" sort-asc="true"/>
        <column-ref ref="theme" filter="true"/>
        <column-ref ref="correspondent" filter="true"/>
        <column-ref ref="addresseesName" filter="true"/>
        <column-ref ref="authorTemplate" filter="true"/>
      </columns>
    </table>
    <actions>
      <action id="InputDocs-CreateRKKTemplate" name="${catalog.custom-descriptor.InputDocs-Catalog-Template-Input-Doc.actions.action.InputDocs-CreateRKKTemplate.name:Создать/Шаблон входящего документа}">
        <create object-ref="RKK_TEMPLATE_INPUT"/>
      </action>
    </actions>
  </custom-descriptor>
</catalog>