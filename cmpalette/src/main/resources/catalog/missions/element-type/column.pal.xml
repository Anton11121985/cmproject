<?xml version='1.0' encoding='UTF-8'?>
<catalog xmlns="http://www.intertrust.ru/schema/palette/cat-ui">
    <complex-column name="missionsTypeSettingsInfo" extends="object">
        <column-ref ref="id" />
        <column-ref ref="docsType"/>
        <column-ref ref="docsName"/>
    </complex-column>
    <column name="docsType" subject="${catalog.column.docsType.subject:Название шаблона}" extends="string" length="80"/>
    <column name="docsName" subject="${catalog.column.docsName.subject:Название шаблона}" extends="string"/>
    <complex-column display="icon" name="docStatus" subject="${catalog.column.docStatus.subject:Статус документа}" extends="icon">
        <meta>
            <documentation>Статус документа</documentation>
        </meta>
    </complex-column>
    <complex-column name="registrationNumber" subject="${catalog.column.registrationNumber.subject:Рег. номер}" extends="regNumber">
        <meta>
            <documentation>Регистрационный номер</documentation>
        </meta>
    </complex-column>
    <column name="documentDateWN" subject="${catalog.column.documentDateMiss.subject:Дата}" extends="datetime">
        <meta>
            <documentation>Дата</documentation>
        </meta>
    </column>
    <column name="department" subject="${catalog.column.department.subject:СП}" extends="string">
        <meta>
            <documentation>СП</documentation>
        </meta>
    </column>
    <column name="hierDepartment" subject="${catalog.column.hierDepartment.subject:Подразделение}" extends="string">
        <meta>
            <documentation>Подразделение</documentation>
        </meta>
    </column>
    <column name="internalDoc" subject="${catalog.column.branch.subject:ВНД}" extends="string">
        <meta>
            <documentation>ВНД</documentation>
        </meta>
    </column>
    <column name="misProjectNumber" subject="${catalog.column.misProjectNumber.subject:Проектный №}" extends="string">
        <meta>
            <documentation>Проектный №</documentation>
        </meta>
    </column>
    <column name="approvalDate" subject="${catalog.column.approvalDate.subject:Срок согласования}" extends="date">
        <meta>
            <documentation>Срок согласования</documentation>
        </meta>
    </column>
    <column name="approvalCycle" subject="${catalog.column.approvalCycle.subject:Круг согласования}" extends="string">
        <meta>
            <documentation>Номер круга согласования</documentation>
        </meta>
    </column>
    <column name="docStatusString" subject="${catalog.column.docStatusString.subject:Статус документа}" extends="string">
        <meta>
            <documentation>Статус документа</documentation>
        </meta>
    </column>
    <column name="createdNote" subject="${catalog.column.createdNote.subject:Создатель}" extends="string">
        <meta>
            <documentation>Создатель</documentation>
        </meta>
    </column>
    <column name="CreatedDateDoc" subject="${catalog.column.CreatedDateDoc.subject:Дата создания}" extends="datetime">
        <meta>
            <documentation>Дата создания</documentation>
        </meta>
    </column>
</catalog>