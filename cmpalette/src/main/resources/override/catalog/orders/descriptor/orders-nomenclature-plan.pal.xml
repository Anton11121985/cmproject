<?xml version='1.0' encoding='UTF-8'?>
<catalog xmlns="http://www.intertrust.ru/schema/palette/cat-ui">
    <custom-descriptor ident="JobOrders" view="Order_(nomenclature_plan)" id="orders-nomenclature-plan"
                       name="Номенклатурный план" style="default">
        <builder name="ru.intertrust.cm_sochi.srv.uicollections.UiBuilder"/>
        <table>
            <columns>
                <column-ref ref="self"/>
                <column-ref ref="hasAttachments" />
                <column-ref ref="orderNumber" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="nomenclatureName" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="consumerName" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="value" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="vatSum" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="orderDeadline" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="plannedDeadline" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="executorName" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="comment" filter="true" sort-asc="true" sort-desc="true"/>
            </columns>
        </table>
    </custom-descriptor>
</catalog>