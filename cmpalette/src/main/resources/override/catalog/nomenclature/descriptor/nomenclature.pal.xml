<?xml version='1.0' encoding='UTF-8'?>
<catalog xmlns="http://www.intertrust.ru/schema/palette/cat-ui">
    <custom-descriptor ident="Nomenclature" view="Nomenclature_(default)" id="nomenclature"
                       name="Номенклатура" style="default">
        <builder name="ru.intertrust.cm_sochi.srv.uicollections.UiBuilder"/>
        <table>
            <columns>
                <column-ref ref="self"/>
                <column-ref ref="nomenclatureCode" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="nomenclatureName" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="nomenclaturePrice" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="nomenclatureVatPrice" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="nomenclatureUnit" filter="true" sort-asc="true" sort-desc="true"/>
            </columns>
        </table>
        <custom-actions schema-ref="NomenclatureActions"/>
    </custom-descriptor>
    <custom-descriptor ident="Nomenclature" view="Nomenclature_(link)" id="nomenclature-link"
                       name="Номенклатура" style="default">
        <builder name="ru.intertrust.cm_sochi.srv.uicollections.UiBuilder"/>
        <table>
            <columns>
                <column-ref ref="self"/>
                <column-ref ref="objectLink"/>
                <column-ref ref="nomenclatureCode" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="nomenclatureName" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="nomenclaturePrice" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="nomenclatureVatPrice" filter="true" sort-asc="true" sort-desc="true"/>
                <column-ref ref="nomenclatureUnit" filter="true" sort-asc="true" sort-desc="true"/>
            </columns>
        </table>
    </custom-descriptor>
</catalog>