<?xml version='1.0' encoding='UTF-8'?>
<catalog xmlns="http://www.intertrust.ru/schema/palette/cat-ui">
  <custom-descriptor ident="ExampleDoc" view="ExampleDoc_(vw_cmj_all_documents)" id="dp-example-category-hide-empty" name="Скрыть пустые категории" style="default">
    <builder name="ru.intertrust.cm_sochi.srv.uicollections.UiBuilder">
      <config>
        <view-complex-classif hide-empty-categories="true"/>
      </config>
    </builder>
    <table>
      <columns>
        <column-ref ref="self"/>
        <column-ref ref="exampleStr" category="true" filter="true"/>
        <column-ref ref="subject" category="true" show-count="true" filter="true"/>
        <column-ref ref="hasAttachments"/>
        <column-ref ref="exampleIcon"/>
        <column-ref ref="exampleInt" important="true"/>
        <column-ref ref="exampleDbl" stretch="10" important="true"/>
        <column-ref ref="exampleBln" filter="true"/>
        <column-ref ref="exampleDTR" important="true"/>
        <column-ref ref="exampleDT" filter="true"/>
        <column-ref ref="exampleDate" />
        <column-ref ref="exampleTime" invisible-default="true"/>
        <column-ref ref="exampleEnumStr" stretch="10"/>
        <column-ref ref="exampleEmail" filter="true"/>
        <column-ref ref="exampleActor"/>
        <column-ref ref="exampleHyperLink" invisible-default="true"/>
      </columns>
    </table>
    <custom-actions schema-ref="ExampleClassifierCollectionRootMenu"/>
  </custom-descriptor>
</catalog>