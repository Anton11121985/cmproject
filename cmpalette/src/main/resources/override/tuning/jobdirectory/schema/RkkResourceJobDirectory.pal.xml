<?xml version='1.0' encoding='UTF-8'?>
<schemas xmlns="http://www.intertrust.ru/schema/palette/tn-schema">
  <schema extends="RkkResource" name="RkkResourceJobDirectory">
    <string name="showIdent" temporal="true">
      <calc-values>
        <script event="open" lang="SPEL"><![CDATA['Справочник рабочих мест']]></script>
      </calc-values>
    </string>
    <string name="Country">
      <storage-field name="Country"/>
    </string>
    <string name="Locality">
      <storage-field name="Locality"/>
    </string>
    <string name="Address">
      <storage-field name="Address"/>
    </string>
    <string name="Floors">
      <storage-field name="Floors"/>
    </string>
    <string name="Rooms">
      <storage-field name="Rooms"/>
    </string>
    <boolean name="$hideUnusedActions" temporal="true">
      <meta>
        <documentation>Управление видимостью системных кнопок</documentation>
      </meta>
      <calc-values>
        <script event="open" lang="SPEL"><![CDATA[
(link.?[rel=='http://intertrust.ru/cmj/relations#applyTemplateButton'].size()>0 ?
link.remove(link.?[rel=='http://intertrust.ru/cmj/relations#applyTemplateButton'].get(0)) : true)]]></script>
      </calc-values>
    </boolean>
  </schema>
