<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="RkkResourceMeetingsAL" id="RkkResourceMeetingsALForm" projection="default" showtabs="true">
    <appearance widget="tabs"/>
    <component>
      <section id="Header">
        <appearance field-layout="horizontal"/>
        <component>
          <field attribute-ref="showIdent">
            <properties>
              <read-only condition-union="AND">
                <condition>true</condition>
              </read-only>
            </properties>
            <appearance align="left" importance="normal">
              <label-style field-width="4" field-unit="symbol"/>
              <alt-appearancies>
                <alt-appearance>
                  <appearance-ref ref="css_headerElementWithIcon"/>
                  <enable condition-union="AND">
                    <condition>true</condition>
                  </enable>
                </alt-appearance>
              </alt-appearancies>
            </appearance>
          </field>
        </component>
      </section>

      <section id="Attributes" name="${ui.form.RkkResourceMeetingsALForm.component.section.Attributes.name:Реквизиты}">
        <component>
          <field attribute-ref="planning" name="${ui.form.RkkResourceMeetingsALForm.component.section.Attributes.component.field.planning.name:Планирование:}" />
          <field attribute-ref="link" name="${ui.form.RkkResourceMeetingsALForm.component.section.Attributes.component.field.link.name:Ссылка}" />
          <field attribute-ref="meetingPlace" name="${ui.form.RkkResourceMeetingsALForm.component.section.Attributes.component.field.meetingPlace.name:Место встречи}" />
          <field attribute-ref="meetingDate" name="${ui.form.RkkResourceMeetingsALForm.component.section.Attributes.component.field.meetingDate.name:Дата встречи}" />
          <field attribute-ref="meetingTime" name="${ui.form.RkkResourceMeetingsALForm.component.section.Attributes.component.field.meetingTime.name:Время встречи}" />
          <field attribute-ref="file" name="${ui.form.RkkResourceMeetingsALForm.component.section.Attributes.component.field.file.name:Файлы}" />
        </component>
      </section>
    </component>
  </form>
