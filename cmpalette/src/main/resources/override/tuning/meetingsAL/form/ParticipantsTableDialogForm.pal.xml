<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
    <form id="ParticipantsTableDialogForm" schema-ref="ParticipantsTable" projection="default" name="${ui.form.ParticipantsTableDialogForm.name:Участник встречи}">
        <properties>
            <hide condition-union="AND">
                <negative-condition-ref ref="dialog"/>
            </hide>
        </properties>
        <component>
            <field attribute-ref="Participant" name="${ui.form.ParticipantsTableDialogForm.Correspondent.name:Участник:}">
                <directories limit-livesearch="no-restrictions">
                    <directory-ref ref="NONSYS_HUMAN"/>
                </directories>
            </field>
            <field attribute-ref="Organization" name="${ui.form.ParticipantsTableDialogForm.Index.name:Организация:}"/>
            <field attribute-ref="Position" name="${ui.form.ParticipantsTableDialogForm.SendAddress.name:Должность:}"/>
            <field attribute-ref="Phone" name="${ui.form.ParticipantsTableDialogForm.field.Recipient.name:Телефон:}"/>
            <field attribute-ref="E-mail" name="${ui.form.ParticipantsTableDialogForm.Phone.name:E-mail:}"/>
        </component>
    </form>
</ui>