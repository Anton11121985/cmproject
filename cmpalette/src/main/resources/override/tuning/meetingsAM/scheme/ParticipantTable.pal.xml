<?xml version='1.0' encoding='UTF-8'?>
<schemas xmlns="http://www.intertrust.ru/schema/palette/tn-schema">
    <schema recalc="true" name="ParticipantTable">
        <!--Участник-->
        <actor name="Participant" required="true">
            <storage-field name="Participant"/>
        </actor>
        <!--Организация-->
        <string name="Organization" array="true">
            <storage-field name="Organization"/>
        </string>
        <!--Должность-->
        <string name="Position" array="true">
            <storage-field name="Position"/>
        </string>
        <!--Телефон-->
        <string name="Phone" array="true">
            <storage-field name="Phone"/>
        </string>
        <!--Email-->
        <string name="Email" array="true">
            <storage-field name="Email"/>
        </string>
    </schema>
</schemas>