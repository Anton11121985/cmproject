<?xml version='1.0' encoding='UTF-8'?>
<schemas xmlns="http://www.intertrust.ru/schema/palette/tn-schema">
    <schema name="LockerLog">
        <meta>
            <category name="ЭП для НР-операции"/>
        </meta>
        <schema-ref null-empty="true" ref="CertificateData" name="certificate" temporal="true"/>
        <schema-ref null-empty="true" ref="LockerClientData" name="locker" temporal="true"/>
        <schema-ref null-empty="true" ref="SignatureData" name="signature" temporal="true"/>
        <datetime name="operation_time" temporal="true"/>
        <integer name="result" temporal="true"/>
        <integer name="error_code" temporal="true"/>
        <string name="error_text" temporal="true"/>
    </schema>
</schemas>