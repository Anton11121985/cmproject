<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <condition name="tempContentNotEmpty">
    <schema-ref ref="RkkTemplateResource" condition-union="AND">
      <boolean-ref value="true" ref="contentNotEmpty"/>
    </schema-ref>
  </condition>
  <condition name="tempContentAddNotEmpty">
    <schema-ref ref="RkkTemplateResource" condition-union="AND">
      <boolean-ref value="true" ref="contentAddNotEmpty"/>
    </schema-ref>
  </condition>
  <condition name="ShowSecurityStampFieldTemplateInput">
    <schema-ref ref="RkkTemplateResourceInput" condition-union="AND">
      <boolean-ref value="true" ref="showSecurityStampField"/>
    </schema-ref>
  </condition>
</ui>