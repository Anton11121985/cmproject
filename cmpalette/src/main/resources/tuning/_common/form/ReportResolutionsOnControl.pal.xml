<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="ReportResolutionsOnControl" id="ReportResolutionsOnControl" projection="default">
    <properties>
      <hide condition-union="AND">
        <negative-condition-ref ref="edit"/>
        <negative-condition-ref ref="read"/>
        <negative-condition-ref ref="dialog"/>
      </hide>
    </properties>
    <component>
      <button name="${ui.form.ReportResolutionsOnControl.button.Построить отчет.name:Построить отчет}">
        <appearance>
          <label-style>
            <style id="tunBtnBlue"/>
          </label-style>
        </appearance>
        <action>
          <system name="http://intertrust.ru/cmj/relations#start"/>
        </action>
      </button>
      <field attribute-ref="EMPL.Value" name="${ui.form.ReportResolutionsOnControl.field.EMPL.Value.name:Исполнитель}">
        <properties>
          <require condition-union="AND">
            <condition>true</condition>
          </require>
        </properties>
        <appearance importance="normal"/>
        <directories limit-livesearch="sys-current">
          <directory-ref ref="CURRENT_SYS_HUMAN"/>
          <directory-ref ref="current_all_with_org_hier"/>
          <directory-ref ref="sys_roles"/>
          <directory-ref ref="FOREIGN_ORGS"/>
          <directory-ref ref="FV_CURR_ALL"/>
        </directories>
      </field>
    </component>
  </form>
</ui>