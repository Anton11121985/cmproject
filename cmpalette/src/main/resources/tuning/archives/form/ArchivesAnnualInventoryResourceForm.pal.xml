                           <directory-ref ref="archivesKindDeals" />
                         </directories>
                       </field>
                       <section id="SectionReadyToConsider" group="true">
                         <appearance field-layout="horizontal" align="right"/>
                         <component>
                           <button name="${ui.form.ArchivesAnnualInventoryResourceForm.label.unreadyToConsider.name:Не готова к ЭК}">
