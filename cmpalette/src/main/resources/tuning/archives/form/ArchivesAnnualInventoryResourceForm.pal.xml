                               <custom>
                                 <operation>
                                   <handler>
                                     <script lang="JavaBean">ToggleReadyToConsider</script>
                                   </handler>
                                 </operation>
                               </custom>
                             </action>
                           </button>
                           <button recalc="true" attribute-ref="readyToConsider" name="${ui.form.ArchivesAnnualInventoryResourceForm.label.unreadyToConsider.name:Не готова к ЭК}">
                             <properties>
                               <hide condition-union="AND">
