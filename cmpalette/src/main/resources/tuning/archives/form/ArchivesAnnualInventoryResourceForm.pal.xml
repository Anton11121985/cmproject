                                 <style id="tunRed" />
                               </label-style>
                             </appearance>
                           </button>
                           <button recalc="true" attribute-ref="readyToConsider" name="${ui.form.ArchivesAnnualInventoryResourceForm.label.readyToConsider.name:Готова к ЭК}">
                             <properties>
                               <hide>
                                 <negative-condition-ref ref="AnnualInventory_isReadyToConsider"/>
