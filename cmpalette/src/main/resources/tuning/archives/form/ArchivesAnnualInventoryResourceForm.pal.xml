              <hide>
                <negative-condition-ref ref="ArchivesAnnualInventory_isSuperAdminAndDeleted"/>
              </hide>
            </properties>
            <action position="30">
              <custom>
                <operation>
                  <handler-ref ref="DeletePermanentlyArchive"/>
                </operation>
              </custom>
            </action>
          </button>
        </component>
