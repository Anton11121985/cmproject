                               <disabled>
                                 <negative-condition-ref ref="AnnualInventory_isDisabledReadyToConsider"/>
                               </disabled>
                             </properties>
                             <appearance>
                               <label-style>
                                 <style id="tunBtnHeader" />
                                 <style id="tunGreen" />
                               </label-style>
                             </appearance>
                           </button>
                         </component>
                       </section>
                     </component>
                   </section>

                    <!--HeaderRow20 section content-->
                 </component>
                 <appearance field-layout="horizontal" />
