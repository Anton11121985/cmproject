                              <hide>
                                <negative-condition-ref ref="AnnualInventory_isConfirmAPKArcOrg"/>
                              </hide>
                              <disabled>
                                <negative-condition-ref ref="AnnualInventory_isConfirmAPKArcOrgOn"/>
                              </disabled>
                            </properties>
                            <appearance>
                              <label-style>
                                <style id="tunBtnHeader" />
                                <style id="tunGreen" />
                              </label-style>
                            </appearance>
                          </button>
                        </component>
                       </section>
                     </component>
                   </section>
                    <field attribute-ref="protocolApkDate" name="${ui.form.ArchivesAnnualInventoryResourceForm.section.Header.section.HeaderRow30.field.protocolApkDate.name:Протокол ЭПК от}">
                      <properties>
