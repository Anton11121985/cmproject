                </directories>
                <appearance importance="high" multiline="false" align="right" />
              </field>
              <field attribute-ref="additionalState">
                <properties>
                  <read-only condition-union="AND">
                    <condition>true</condition>
                  </read-only>
                </properties>
                <directories>
                  <directory-ref ref="archivesAnnualInventoryStateAdditionalState" />
                </directories>
                <appearance importance="high" multiline="false" align="right" />
              </field>
              <field attribute-ref="$approvStatusInfo">
                <appearance align="right"/>
              </field>
            </component>
