          </field>
          <field attribute-ref="contentDocuments.content">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="archivesAnnualInventory_contentDocumentsEmpty"/>
                <negative-condition-ref ref="edit"/>
              </hide>
            </properties>
            <appearance importance="normal"/>
          </field>
        </component>
