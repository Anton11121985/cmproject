                            </properties>
                            <appearance>
                             <label-style>
                               <style id="tunBtnHeader" />
                               <style id="tunRed" />
                             </label-style>
                            </appearance>
                          </button>
                          <button recalc="true" attribute-ref="isConfirmAPKArcOrg" name="${ui.form.ArchivesAnnualInventoryResourceForm.label.isConfirmAPKArcOrg.name:Утвержден госархивом}">
