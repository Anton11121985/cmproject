              <hide condition-union="AND">
                <negative-condition-ref ref="AnnualInventory_canEdit"/>
              </hide>
            </properties>
            <icon code="95"/>
            <action position="4.0">
              <custom>
                <open-edit/>
              </custom>
            </action>
          </button>
          <button name="${ui.form.RkkResourceDTRForm.button.ReceiveDocs.name:Скрыть Редактировать}">
            <properties>
              <hide>
                <condition>true</condition>
              </hide>
            </properties>
            <action>
              <system name="EDIT"/>
            </action>
          </button>
          <button name="${ui.form.ArchivesAnnualInventoryResourceForm.section.RootMenu.button.Сохранить скрыта системная.name:Сохранить скрыта системная}">
            <properties>
              <hide condition-union="AND">
                <condition>true</condition>
              </hide>
            </properties>
            <action>
              <system name="SAVE" />
            </action>
          </button>
          <button name="${ui.form.ArchivesAnnualInventoryResourceForm.section.RootMenu.button.Сохранить.name:Сохранить}">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="archivesAnnualInventory_IsShowSaveButtonForNew" />
              </hide>
            </properties>
            <appearance>
              <label-style>
                <style id="tunBtnHeader" />
                <style id="tunIconSave" />
              </label-style>
            </appearance>
            <action>
              <custom>
                <save />
              </custom>
            </action>
          </button>
          <button name="${ui.form.ArchivesAnnualInventoryResourceForm.section.RootMenu.button.Сохранить.name:Сохранить}">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="archivesAnnualInventory_IsNotNew" />
              </hide>
            </properties>
            <appearance>
              <label-style>
                <style id="tunBtnHeader" />
                <style id="tunIconSave" />
              </label-style>
            </appearance>
            <action>
              <custom>
                <save-existing />
              </custom>
            </action>
          </button>
          <button name="${ui.form.NdpRegisterForm.button.canSendToApproving.name:Вернуть в доработку}">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="Archives_CanReturnToRevisionAnnInv"/>
              </hide>
            </properties>
            <hint-pop-up>${ui.form.NdpRegisterForm.button.canSendToApproving.hintPopUp:Вернуть в доработку}</hint-pop-up>
            <action position="9.0">
              <custom>
                <operation>
                  <handler>
                    <script lang="JavaBean">ReturnToRevisionAnnualInventory</script>
                  </handler>
                </operation>
              </custom>
            </action>
          </button>
          <button name="${ui.form.NdoRegisterForm.section.RootMenu.button.ComposeFinalNote.name:Составить итоговую запись}">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="canSeeButtonComposeFinalNoteDepDeal"/>
              </hide>
            </properties>
            <appearance align="left">
              <label-style width="20">
                <style id="tunUnderline" />
                <style id="tunClickableRed" />
              </label-style>
            </appearance>
            <action>
              <custom>
                <operation>
                  <handler>
                    <script lang="JavaBean">ComposeFinalNoteForAnnInv</script>
                  </handler>
                </operation>
              </custom>
            </action>
          </button>
          <button name="${ui.form.ArchivesDocResourceForm.section.RootMenu.button.Verify.name:Заверить}">
            <properties>
              <hide condition-union="AND">
                <negative-condition-ref ref="AnnInv_canVerify" />
              </hide>
            </properties>
            <action position="20">
              <custom>
                <operation>
                  <handler>
                    <script lang="JavaBean">AnnualInventoryVerify</script>
                  </handler>
                </operation>
              </custom>
            </action>
          </button>
