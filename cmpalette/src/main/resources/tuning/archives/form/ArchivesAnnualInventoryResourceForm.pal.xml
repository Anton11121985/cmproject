                            <properties>
                             <hide>
                               <condition-ref ref="AnnualInventory_isConfirmAPKArcOrg" />
                             </hide>
                             <disabled>
                               <negative-condition-ref ref="AnnualInventory_isConfirmAPKArcOrgOff"/>
