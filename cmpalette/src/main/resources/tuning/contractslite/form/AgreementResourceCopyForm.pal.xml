<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="AgreementResource" id="AgreementResourceCopyForm" projection="default">
    <properties>
      <hide condition-union="AND">
        <negative-condition-ref ref="copy"/>
      </hide>
    </properties>
    <component>
      <field attribute-ref="disableLinkAccess" name="${ui.form.AgreementResourceCopyForm.field.disableLinkAccess.name:Ограничить доступ по связям}">
        <appearance align="right" importance="normal">
          <label-style field-width="2" field-unit="symbol"/>
        </appearance>
        <hint-in-field>${ui.form.AgreementResourceCopyForm.field.disableLinkAccess.hintInField:Не дается доступ к текущему документу читателям связанных документов}</hint-in-field>
      </field>
      <field attribute-ref="category" name="${ui.form.AgreementResourceCopyForm.field.category.name:Тематика:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.category.hintPopUp:Тематика}</hint-pop-up>
      </field>
      <field attribute-ref="value.currency" name="${ui.form.AgreementResourceCopyForm.field.value.currency.name:Тип валюты:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.value.currency.hintPopUp:Тип валюты}</hint-pop-up>
      </field>
      <field attribute-ref="value.advancepayment" name="${ui.form.AgreementResourceCopyForm.field.value.advancepayment.name:Предоплата:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.value.advancepayment.hintPopUp:Предоплата}</hint-pop-up>
      </field>
      <field attribute-ref="signatureContracts.other.signer" name="${ui.form.AgreementResourceCopyForm.field.signatureContracts.other.signer.name:Подпись (2-я сторона):}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.signatureContracts.other.signer.hintPopUp:Подпись (2-я сторона)}</hint-pop-up>
      </field>
      <field attribute-ref="started" name="${ui.form.AgreementResourceCopyForm.field.started.name:Дата начала работ:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.started.hintPopUp:Дата начала работ}</hint-pop-up>
      </field>
      <field attribute-ref="finished" name="${ui.form.AgreementResourceCopyForm.field.finished.name:Дата завершения работ:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.finished.hintPopUp:Дата завершения работ}</hint-pop-up>
      </field>
      <field attribute-ref="duration" name="${ui.form.AgreementResourceCopyForm.field.duration.name:Срок действия:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.duration.hintPopUp:Срок действия}</hint-pop-up>
      </field>
      <field attribute-ref="preparedBy" name="${ui.form.AgreementResourceCopyForm.field.preparedBy.name:Подготовили:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.preparedBy.hintPopUp:Подготовили}</hint-pop-up>
      </field>
      <field attribute-ref="responsibles" name="${ui.form.AgreementResourceCopyForm.field.responsibles.name:Ответственные за исполнение:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.responsibles.hintPopUp:Ответственные за исполнение}</hint-pop-up>
      </field>
      <field attribute-ref="sendlist" name="${ui.form.AgreementResourceCopyForm.field.sendlist.name:Список рассылки:}">
        <appearance importance="normal">
          <field-style group="true"/>
        </appearance>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.sendlist.hintPopUp:Список рассылки}</hint-pop-up>
      </field>
      <field attribute-ref="pages" name="${ui.form.AgreementResourceCopyForm.field.pages.name:Кол-во листов:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.pages.hintPopUp:Кол-во листов}</hint-pop-up>
      </field>
      <field attribute-ref="copies" name="${ui.form.AgreementResourceCopyForm.field.copies.name:Кол-во экземпляров:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.copies.hintPopUp:Кол-во экземпляров}</hint-pop-up>
      </field>
      <field attribute-ref="appendices" name="${ui.form.AgreementResourceCopyForm.field.appendices.name:Кол-во приложений:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.appendices.hintPopUp:Кол-во приложений}</hint-pop-up>
      </field>
      <field attribute-ref="appendicesPages" name="${ui.form.AgreementResourceCopyForm.field.appendicesPages.name:Листов приложения:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.appendicesPages.hintPopUp:Листов приложения}</hint-pop-up>
      </field>
      <field attribute-ref="value.total" name="${ui.form.AgreementResourceCopyForm.field.value.total.name:Сумма дополнительного соглашения:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.value.total.hintPopUp:Сумма дополнительного соглашения}</hint-pop-up>
      </field>
      <field attribute-ref="value.scheme" name="${ui.form.AgreementResourceCopyForm.field.value.scheme.name:Порядок оплаты:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.AgreementResourceCopyForm.field.value.scheme.hintPopUp:Порядок оплаты}</hint-pop-up>
      </field>
      <field attribute-ref="$hasContentWithAffirm">
        <appearance importance="normal"/>
      </field>
    </component>
  </form>
</ui>