          <hide condition-union="AND">
            <negative-condition-ref ref="isContractProject"/>
          </hide>
        </properties>
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.concluded.hintPopUp:Плановая дата заключения}</hint-pop-up>
      </field>
      <field attribute-ref="started" name="${ui.form.RkkResourceContractsCopyForm.field.started.name:Дата начала договора:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.started.hintPopUp:Дата начала договора}</hint-pop-up>
      </field>
      <field attribute-ref="finished" name="${ui.form.RkkResourceContractsCopyForm.field.finished.name:Дата завершения работ:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.finished.hintPopUp:Дата завершения работ}</hint-pop-up>
      </field>
      <field attribute-ref="duration" name="${ui.form.RkkResourceContractsCopyForm.field.duration.name:Срок действия:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.duration.hintPopUp:Срок действия}</hint-pop-up>
