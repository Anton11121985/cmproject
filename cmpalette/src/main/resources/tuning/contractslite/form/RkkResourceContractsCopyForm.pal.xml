      </field>
      <field attribute-ref="parties.other" name="${ui.form.RkkResourceContractsCopyForm.field.parties.other.name:Вторая сторона:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.parties.other.hintPopUp:Вторая сторона}</hint-pop-up>
      </field>
      <field attribute-ref="advSigner" name="${ui.form.RkkResourceContractsCopyForm.field.advSigner.name:Другие подписи (1-я сторона):}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.advSigner.hintPopUp:Другие подписи (1-я сторона)}</hint-pop-up>
        <directories limit-livesearch="sys-current">
          <directory-select-depart attribute-ref="parties.this" ref="CURRENT_SYS_ORGS_DEPS"/>
          <directory-select-depart attribute-ref="parties.this" ref="ALIEN_BEARDS"/>
        </directories>
      </field>
      <field attribute-ref="relationsType" name="${ui.form.RkkResourceContractsCopyForm.field.relationsType.name:Тип отношений:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.relationsType.hintPopUp:Тип отношений}</hint-pop-up>
      </field>
      <field attribute-ref="departmentPrep" name="${ui.form.RkkResourceContractsCopyForm.field.departmentPrep.name:Подразделение, отв. за подготовку:}">
