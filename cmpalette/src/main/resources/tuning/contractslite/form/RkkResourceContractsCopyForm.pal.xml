        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.comment.hintPopUp:Комментарий}</hint-pop-up>
      </field>
      <field attribute-ref="disableLinkAccess" name="${ui.form.RkkResourceContractsCopyForm.field.disableLinkAccess.name:Ограничить доступ по связям:}">
        <appearance align="right" importance="normal">
          <label-style field-width="2" field-unit="symbol"/>
        </appearance>
        <hint-in-field>${ui.form.RkkResourceContractsCopyForm.field.disableLinkAccess.hintInField:Не дается доступ к текущему документу читателям связанных документов}</hint-in-field>
