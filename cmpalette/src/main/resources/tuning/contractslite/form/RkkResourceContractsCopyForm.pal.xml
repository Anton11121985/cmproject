        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.value.currency.hintPopUp:Тип валюты}</hint-pop-up>
      </field>
      <field attribute-ref="value.total" name="${ui.form.RkkResourceContractsCopyForm.field.value.total.name:Сумма договора:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.value.total.hintPopUp:Сумма договора}</hint-pop-up>
      </field>
      <field attribute-ref="$hasContentWithAffirm">
        <appearance importance="normal"/>
      </field>
    </component>
  </form>
</ui>