        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.otherparties.hintPopUp:Другие стороны}</hint-pop-up>
      </field>
      <field attribute-ref="value.advancepayment" name="${ui.form.RkkResourceContractsCopyForm.field.value.advancepayment.name:Предоплата:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.value.advancepayment.hintPopUp:Предоплата}</hint-pop-up>
      </field>
      <field attribute-ref="value.scheme" name="${ui.form.RkkResourceContractsCopyForm.field.value.scheme.name:Порядок оплаты:}">
