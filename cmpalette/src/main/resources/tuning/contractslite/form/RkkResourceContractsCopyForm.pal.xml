<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="RkkResourceContracts" id="RkkResourceContractsCopyForm" projection="default">
    <properties>
      <hide condition-union="AND">
        <negative-condition-ref ref="copy"/>
      </hide>
    </properties>
    <component>
      <field attribute-ref="title">
        <appearance importance="normal"/>
      </field>
      <field attribute-ref="type" name="${ui.form.RkkResourceContractsCopyForm.field.type.name:Тип договора:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.type.hintPopUp:Тип договора}</hint-pop-up>
      </field>
      <field attribute-ref="value.source" name="${ui.form.RkkResourceContractsCopyForm.field.value.source.name:Источник финансирования:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.value.source.hintPopUp:Источник финансирования}</hint-pop-up>
      </field>
      <field attribute-ref="value.kbk" name="${ui.form.RkkResourceContractsCopyForm.field.value.kbk.name:Коды расходов:}">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.value.kbk.hintPopUp:Коды расходов}</hint-pop-up>
      </field>
      <field attribute-ref="additionalnumber" name="${ui.form.RkkResourceContractsCopyForm.field.additionalnumber.name:Доп. Номер:}">
