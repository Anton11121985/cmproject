        <hint-pop-up>${ui.form.RkkResourceContractsCopyForm.field.responsibles.hintPopUp:Ответственные за исполнение}</hint-pop-up>
      </field>
      <field attribute-ref="sendlist" name="${ui.form.RkkResourceContractsCopyForm.field.sendlist.name:Список рассылки:}">
