<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
    <form schema-ref="AddManuallyDocResource" id="AddManuallyDocResourceSubform" projection="default" group="true">
