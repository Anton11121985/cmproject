<?xml version='1.0' encoding='UTF-8'?>
<schemas xmlns="http://www.intertrust.ru/schema/palette/tn-schema">
    <schema name="DTRInsertBarcodeDialog" extends="Dialog">
        <meta>
            <category name="Диалоги"/>
            <documentation>Схема диалога для ввода штрихкода модуля "Реестр передачи документов"</documentation>
        </meta>
        <string name="counter"/>
        <string name="label">
            <calc-values>
                <script event="open" lang="SPEL" ><![CDATA['Отсканируйте штрихкод или введите его код в поле']]></script>
            </calc-values>
        </string>
        <string name="barCode"/>
        <boolean name="barCodeNotIsEmpty">
            <calc-values>
                <script event="open" lang="SPEL" ><![CDATA[barCode!=null && !barCode.equals("")]]></script>
            </calc-values>
        </boolean>

    </schema>
</schemas>