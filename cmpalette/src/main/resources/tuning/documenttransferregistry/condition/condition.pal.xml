        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_isDeleted">
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <boolean-ref value="true" ref="isDeleted"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_canEditSenderDep">
        <modes>
            <mode name="edit"/>
        </modes>
        <schema-ref condition-union="AND" ref="RkkResourceDTR">
            <boolean-ref value="true" ref="canEditSenderDep"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_editIsNotNewNotTransferred">
        <modes>
            <mode name="edit"/>
        </modes>
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <boolean-ref value="false" ref="wasTransferred"/>
            <boolean-ref value="false" ref="isNew"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_editTransferred">
        <modes>
            <mode name="edit"/>
        </modes>
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <boolean-ref value="true" ref="wasTransferred"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_canSendRegistry">
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <boolean-ref value="true" ref="isSender"/>
            <string-ref value="Проект" ref="state"/>
            <boolean-ref value="true" ref="isReceiverDepNotEmpty"/>
            <boolean-ref value="false" ref="isDeleted"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_canReceive">
        <modes>
            <mode name="read"/>
        </modes>
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <boolean-ref value="true" ref="isReceiver"/>
            <string-ref value="Передано" ref="state"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_canFinishReceiving">
        <modes>
            <mode name="edit"/>
        </modes>
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <boolean-ref value="true" ref="$canFinishReceiving"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_isProject">
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <string-ref value="Проект" ref="state"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_canAddEntryToRegistry">
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <boolean-ref value="true" ref="isSender"/>
            <string-ref value="Проект" ref="state"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_notFoundDocByBarcode">
        <schema-ref condition-union="AND" ref="RkkResourceDTR">
            <boolean-ref value="true" ref="notFoundDocByBarcode"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_canAddEntryForReceive">
        <modes>
            <mode name="edit"/>
        </modes>
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <boolean-ref value="true" ref="isReceiver"/>
            <string-ref value="Передано" ref="state"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_notGetDocByBarcode">
        <schema-ref condition-union="AND" ref="RkkResourceDTR">
            <boolean-ref value="true" ref="notGetDocByBarcode"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_isSender">
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <boolean-ref value="true" ref="isSender"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_isReceiver">
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <boolean-ref value="true" ref="isReceiver"/>
        </schema-ref>
    </condition>
    <condition name="RkkResourceDTR_canDelete">
        <schema-ref ref="RkkResourceDTR" condition-union="AND">
            <boolean-ref value="true" ref="canDelete"/>
        </schema-ref>
    </condition>
