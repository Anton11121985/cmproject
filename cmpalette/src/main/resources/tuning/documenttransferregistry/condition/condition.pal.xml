        <schema-ref ref="DTRReceiveDocumentsDialog" condition-union="AND">
            <!--<boolean-ref ref="isEntriesSelected" value="true"/>
            <string-ref ref="selectionMode" value="requisites"/>-->
            <boolean-ref ref="showMarkReceivedButton" value="true"/>
        </schema-ref>
    </condition>
