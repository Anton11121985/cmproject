            <boolean-ref value="true" ref="canEdit"/>
        </schema-ref>
    </condition>
    <condition name="DTREntrySubObject_canCancelReceiving">
        <schema-ref ref="DTREntrySubObject" condition-union="AND">
            <boolean-ref value="true" ref="$canCancelReceiving"/>
        </schema-ref>
    </condition>
    <condition name="DTREntrySubObject_canNotEdit">
        <schema-ref ref="DTREntrySubObject" condition-union="AND">
            <boolean-ref value="false" ref="canEdit"/>
        </schema-ref>
    </condition>
    <condition name="DTREntrySubObject_createdByHand">
        <schema-ref ref="DTREntrySubObject" condition-union="AND">
            <boolean-ref ref="byHand" value="true"/>
        </schema-ref>
    </condition>
    <condition name="DTREntrySubObject_canNotChangeDeliveryStatus">
        <schema-ref ref="DTREntrySubObject" condition-union="OR">
            <boolean-ref value="false" ref="canEdit"/>
            <boolean-ref value="false" ref="$canCancelReceiving"/>
        </schema-ref>
    </condition>
    <condition name="DTREntrySubObject_isNotReceived">
        <schema-ref ref="DTREntrySubObject" condition-union="AND">
            <string-ref value="Не получено" ref="deliveryStatus"/>
        </schema-ref>
    </condition>
    <condition name="DTREntrySubObject_canEditOrDelete">
        <modes>
            <mode name="edit"/>
            <mode name="table_edit"/>
        </modes>
        <schema-ref ref="DTREntrySubObject" condition-union="OR">
            <boolean-ref value="true" ref="canEdit"/>
            <boolean-ref value="true" ref="canDelete"/>
        </schema-ref>
    </condition>
    <condition name="DTREntrySubObject_canDelete">
        <schema-ref ref="DTREntrySubObject" condition-union="AND">
            <boolean-ref value="true" ref="canDelete"/>
        </schema-ref>
    </condition>
