<?xml version='1.0' encoding='UTF-8'?>
<schemas xmlns="http://www.intertrust.ru/schema/palette/tn-schema">
    <schema name="NormalizedAddress">
        <meta>
            <category name="Экспедиции/Заказы"/>
        </meta>
        <string name="normAddressType">
            <storage-field name="normAddressType"/>
            <meta>
                <documentation>Тип адреса</documentation>
            </meta>
        </string>
        <string name="normArea">
            <storage-field name="normArea"/>
            <meta>
                <documentation>Район</documentation>
            </meta>
        </string>
        <string name="normBuilding">
            <storage-field name="normBuilding"/>
            <meta>
                <documentation>Часть здания: Строение</documentation>
            </meta>
        </string>
        <string name="normCorpus">
            <storage-field name="normCorpus"/>
            <meta>
                <documentation>Часть здания: Корпус</documentation>
            </meta>
        </string>
        <string name="normHotel">
            <storage-field name="normHotel"/>
            <meta>
                <documentation>Название гостиницы</documentation>
            </meta>
        </string>
        <string name="normHouse">
            <storage-field name="normHouse"/>
            <meta>
                <documentation>Часть адреса: Номер здания</documentation>
            </meta>
        </string>
        <string name="normIndex">
            <storage-field name="normIndex"/>
            <meta>
                <documentation>Почтовый индекс</documentation>
            </meta>
        </string>
        <string name="normLetter">
            <storage-field name="normLetter"/>
            <meta>
                <documentation>Часть здания: Литера</documentation>
            </meta>
        </string>
        <string name="normLocation">
            <storage-field name="normLocation"/>
            <meta>
                <documentation>Микрорайон</documentation>
            </meta>
        </string>
        <string name="normNumAddressType">
            <storage-field name="normNumAddressType"/>
            <meta>
                <documentation>Номер для а/я, войсковая часть, войсковая часть ЮЯ, полевая почта</documentation>
            </meta>
        </string>
        <string name="normOriginalAddress" createlog="true">
            <storage-field name="normOriginalAddress"/>
            <meta>
                <documentation>Оригинальный адрес одной строкой</documentation>
            </meta>
        </string>
        <string name="normPlace">
            <storage-field name="normPlace"/>
            <meta>
                <documentation>Населенный пункт</documentation>
            </meta>
        </string>
        <string name="normQualityCode">
            <storage-field name="normQualityCode"/>
            <meta>
                <documentation>Код качества нормализации адреса</documentation>
            </meta>
        </string>
        <string name="normRegion">
            <storage-field name="normRegion"/>
            <meta>
                <documentation>Область, регион</documentation>
            </meta>
        </string>
        <string name="normRoom">
            <storage-field name="normRoom"/>
            <meta>
                <documentation>Часть здания: Номер помещения</documentation>
            </meta>
        </string>
        <string name="normSlash">
            <storage-field name="normSlash"/>
            <meta>
                <documentation>Часть здания: Дробь</documentation>
            </meta>
        </string>
        <string name="normStreet">
            <storage-field name="normStreet"/>
            <meta>
                <documentation>Часть адреса: Улица</documentation>
            </meta>
        </string>
        <string name="normValidationCode">
            <storage-field name="normValidationCode"/>
            <meta>
                <documentation>Код проверки нормализации адреса</documentation>
            </meta>
        </string>
    </schema>
</schemas>