    <component>
      <field attribute-ref="author">
        <appearance multiline="false"/>
      </field>
      <field attribute-ref="createdDate">
        <appearance multiline="false">
          <label-style field-unit="symbol" field-width="15"/>
        </appearance>
      </field>
      <section name="${ui.form.VisaEditorsHistoryForm.section.Открыть.name:Открыть}" id="Открыть">
        <component>
          <field attribute-ref="$content"/>
        </component>
        <appearance style="tunBorderBlueSection externalExpand" turn="true">
          <label-style unit="symbol" width="10">
            <style id="tunBorderBlueSection"/>
            <style id="externalExpand"/>
          </label-style>
          <dialog-mode unit="symbol" width="10"/>
        </appearance>
      </section>
    </component>
    <appearance style="tunCaption tunBorderBottom" field-layout="horizontal">
      <label-style unit="symbol" width="10">
        <style id="tunCaption"/>
        <style id="tunBorderBottom"/>
      </label-style>
    </appearance>
  </form>
</ui>