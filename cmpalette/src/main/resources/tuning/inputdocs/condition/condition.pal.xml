<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <condition name="isCopyMaker">
    <roles>
      <role name="CopyMaker"/>
    </roles>
    <schema-ref ref="" condition-union="AND"/>
  </condition>
  <condition name="movingCopiesEnable">
    <schema-ref ref="RkkResourceInput" condition-union="AND">
      <string-ref value="registered" ref="registration.state.status"/>
      <boolean-ref value="true" ref="movingCopiesEnable"/>
    </schema-ref>
  </condition>
</ui>