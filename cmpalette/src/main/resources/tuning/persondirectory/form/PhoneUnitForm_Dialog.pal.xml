<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="PhoneUnit" id="PhoneUnitForm_Dialog" projection="default">
    <properties>
      <hide condition-union="AND">
        <negative-condition-ref ref="dialog"/>
      </hide>
    </properties>
    <component>
      <field attribute-ref="type" name="${ui.form.PhoneUnitForm_Dialog.field.type.name:Вид телефона}">
        <appearance importance="normal"/>
        <directories manual-input="true" limit-livesearch="no-restrictions">
          <directory-ref ref="PhoneTypes"/>
        </directories>
      </field>
      <field attribute-ref="number" name="${ui.form.PhoneUnitForm_Dialog.field.number.name:Номер телефона}">
        <appearance importance="normal"/>
      </field>
    </component>
  </form>
</ui>