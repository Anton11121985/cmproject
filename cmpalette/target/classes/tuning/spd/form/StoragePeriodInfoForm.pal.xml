<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="StoragePeriodInfo" projection="default" id="StoragePeriodInfoForm">
    <properties>
      <hide>
        <negative-condition-ref ref="digest"/>
      </hide>
    </properties>
    <component>
      <field attribute-ref="period">
        <appearance importance="normal">
          <label-style>
            <style id="tunClickableUnderline"/>
            <style id="tunBlue"/>
          </label-style>
          <field-style>
            <value-style>
              <style id="tunClickableUnderline"/>
              <style id="tunBlue"/>
            </value-style>
          </field-style>
        </appearance>
      </field>
      <field attribute-ref="unid">
        <properties>
          <hide condition-union="AND">
            <condition>true</condition>
          </hide>
        </properties>
        <appearance importance="normal" />
      </field>
      <field attribute-ref="subject">
        <properties>
          <hide condition-union="AND">
            <condition>true</condition>
          </hide>
        </properties>
        <appearance importance="normal" />
      </field>
      <field attribute-ref="articleNum">
        <properties>
          <hide condition-union="AND">
            <condition>true</condition>
          </hide>
        </properties>
        <appearance importance="normal" />
      </field>
      <field attribute-ref="docType">
        <properties>
          <hide condition-union="AND">
            <condition>true</condition>
          </hide>
        </properties>
        <appearance importance="normal" />
      </field>
      <field attribute-ref="epk">
        <properties>
          <hide condition-union="AND">
            <condition>true</condition>
          </hide>
        </properties>
        <appearance importance="normal" />
      </field>
    </component>
  </form>
</ui>