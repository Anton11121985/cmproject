	<parameter name="ndoChange" class="java.lang.Object" isForPrompting="false"/>
	<queryString>
		<![CDATA[select f.id as id, 
	coalesce(f.indexFile, '') as indexFile,
	coalesce(f.titleFile, '') as titleFile,
	coalesce(nullif(f.StoragePeriod, ''),
					(select p.period from SPD_Period p where f.ShelfLife = p.id), '') as StoragePeriod,
	coalesce(nullif(f.SpDescription, ''),
					(select concat(p.NormDocShortName, ' ', p.ArticleNum) from SPD_Period p where f.ShelfLife = p.id), '') as SpDescription,
	coalesce(concat(ur.index, ' - ' ||
				   coalesce(nullif(ur.branchDivision, ''), (select FullName from SO_StructureUnit s where s.beard = ur.sounit), '')
				   ), '') as soUnit,
	coalesce(f_ex.comment, '') as comment
from FR_OrgRegisterChange_Ndps ndps
join nunid2punid_map on nunid = split_part(ndps.fr_unitRegisterUnid, ':', 2)
join FR_UnitRegister ur on substring(punid::text, 1 , 4)::integer = ur.id_type and substring(punid::text, 5 , 12)::integer = ur.id
join FR_UR_File_Register fr on fr.owner = ur.id
join FR_File f on fr.file = f.id
join FR_File_Extended f_ex on f_ex.parent = f.id
where ndps.owner IN (SELECT item FROM QR_Id_List WHERE request = $P{ndoChange});]]>
	</queryString>
