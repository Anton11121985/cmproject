	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="One Empty Record"/>
	<parameter name="EMPL_LIST.Value" class="java.lang.Object"/>
	<parameter name="DATE.Start" class="java.util.Date"/>
	<parameter name="DATE.End" class="java.util.Date"/>
	<parameter name="ExecWhere" class="java.lang.Object">
		<defaultValueExpression><![CDATA[$P{REPORT_SCRIPTLET}.getExecWhere($P{EMPL_LIST.Value})]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[WITH executorsOfInterest AS
         (SELECT executorBeard.id             execId,
                 executorBeard.orig_shortname execName
          FROM so_beard executorBeard
              $P!{ExecWhere}
         ),
/* резолюции */
     resolutionsOfInterest AS
         (SELECT res.id                         resolutId,
                 resExecCurr.executorcurr       execId,
                 executorsOfInterest.execName   execName,
                 resBase.resolution             resText,
                 res.ctrldateexecution          execDate,
                 CASE
                     WHEN exDateCur.execdatecurr BETWEEN '0001.01.02' AND $P{DATE.End} THEN exDateCur.execdatecurr
                     ELSE res.ctrldeadline END AS ctrlDeadline,
                 exDateCur.idx                  execIndex
          FROM f_dp_resolution res
                   JOIN f_dp_resltnbase resBase on res.id = resBase.id
                   JOIN f_dp_resltnbase_execcurr resExecCurr ON resBase.id = resExecCurr.owner
                   JOIN executorsOfInterest ON resExecCurr.executorcurr = executorsOfInterest.execId
                   JOIN F_DP_Resolution_ExDateCur exDateCur ON res.id = exDateCur.owner AND resExecCurr.idx = exdatecur.idx
          WHERE res.isproject = 0
            AND resBase.isdeleted = 0
            AND resBase.ctrliscontrolled = 1
            AND (
              res.ctrldateexecution BETWEEN $P{DATE.Start} AND $P{DATE.End}
                  OR
                  ((res.ctrldateexecution IS NULL OR res.ctrldateexecution > $P{DATE.End})
                  AND res.ctrldeadline < $P{DATE.End})
              )
         ),

     ResolutionsInDocumentT AS (SELECT count(DISTINCT resolutionsOfInterest.resolutId) ResolutionsInDocument FROM resolutionsOfInterest),

     totalResolutionsT AS (SELECT resolutionsOfInterest.execId,
                                  resolutionsOfInterest.execName,
                                  count(DISTINCT resolutionsOfInterest.resolutId) totalResolutions
                           FROM resolutionsOfInterest
                           group by execId,execName),

     totalExecResT AS (SELECT resolutionsOfInterest.execId,
                              count(distinct resolutionsOfInterest.resolutId) totalExecRes
                       FROM resolutionsOfInterest
                       WHERE execDate <= $P{DATE.End}
                       group by execId),

     ExecResDeadMissedT AS (SELECT resolutionsOfInterest.execId,
                                   count(distinct resolutionsOfInterest.resolutId) ExecResDeadMissed
                            FROM resolutionsOfInterest
                            WHERE execDate <= $P{DATE.End}
                                AND ctrlDeadline < execDate
                            group by execId),

     UnexecCtrlRemovedT AS (SELECT resolutionsOfInterest.execId,
                                   count(distinct resolutionsOfInterest.resolutId) UnexecCtrlRemoved
                            from resolutionsOfInterest
                                     LEFT JOIN F_DP_Resolution_ExDOutOwn resExecOut
                                               ON resExecOut.owner = resolutionsOfInterest.resolutId AND
                                                  resolutionsOfInterest.execIndex = resExecOut.idx
                            where (resolutionsOfInterest.execDate IS NULL OR
                                   resolutionsOfInterest.execDate >  $P{DATE.End})
                              AND resExecOut.execdateoutcurr BETWEEN '0001.01.02' AND  $P{DATE.End}
                            group by execId),

    LastReportT AS (SELECT reportId, resolutionParent, reportAuthor, reportDate, reportDeclined, reportApproved
                    FROM (SELECT report.id            reportId,
                                 report.hierparent    resolutionParent,
                                 report.author        reportAuthor,
                                 report.execdate      reportDate,
                                 NULLIF(trim(report.declinewhopid),'') reportDeclined,
                                 report.approvedt     reportApproved,
                                 row_number()
                                 over (PARTITION BY resolutionsOfInterest.execId, resolutionsOfInterest.resolutId ORDER BY report.execdate, report.created_date DESC) rowNumber,
                                 resExecOut.*
                          FROM resolutionsOfInterest
                                   JOIN f_dp_report report ON report.hierparent = resolutionsOfInterest.resolutId
                              AND report.author = resolutionsOfInterest.execId
                                   LEFT JOIN F_DP_Resolution_ExDOutOwn resExecOut
                                             ON resExecOut.owner = resolutionsOfInterest.resolutId AND
                                                resolutionsOfInterest.execIndex = resExecOut.idx
                          WHERE (resolutionsOfInterest.execDate IS NULL OR
                                 resolutionsOfInterest.execDate >  $P{DATE.End})
                            AND report.isdeleted = 0
                            AND report.type = 1
                            AND (resExecOut.id IS NULL OR
                                 resExecOut.execdateoutcurr <  $P{DATE.End} OR
                                 resolutionsOfInterest.execDate >  $P{DATE.End})
                         ) orderedReports
                    WHERE rowNumber = 1),

     UnexecCtrlReportApprovedT AS (SELECT resolutionsOfInterest.execId,
                                          count(distinct resolutionsOfInterest.resolutId) UnexecCtrlReportApproved
                                   from resolutionsOfInterest
                                            JOIN LastReportT
                                                 ON LastReportT.resolutionParent = resolutionsOfInterest.resolutId AND
                                                    LastReportT.reportAuthor = resolutionsOfInterest.execId
                                   where (resolutionsOfInterest.execDate IS NULL OR
                                          resolutionsOfInterest.execDate >  $P{DATE.End})
                                     AND LastReportT.reportId IS NOT NULL
                                     AND reportApproved IS NOT NULL
                                     AND reportDeclined IS NULL
                                   group by execId),


     UnexecCtrlReportDeclinedT AS (SELECT resolutionsOfInterest.execId,
                                          count(distinct resolutionsOfInterest.resolutId) UnexecCtrlReportDeclined
                                   from resolutionsOfInterest
                                            LEFT JOIN LastReportT
                                                      ON LastReportT.resolutionParent = resolutionsOfInterest.resolutId AND
                                                         LastReportT.reportAuthor = resolutionsOfInterest.execId
                                   where (resolutionsOfInterest.execDate IS NULL OR
                                          resolutionsOfInterest.execDate >  $P{DATE.End})
                                     AND (LastReportT.reportId IS NULL
                                       OR reportDeclined IS NOT NULL)
                                   group by execId),

     UnexecCtrlReportMissedT AS (SELECT resolutionsOfInterest.execId,
                                        count(distinct resolutionsOfInterest.resolutId) UnexecCtrlReportMissed
                                 from resolutionsOfInterest
                                          JOIN LastReportT
                                               ON LastReportT.resolutionParent = resolutionsOfInterest.resolutId AND
                                                  LastReportT.reportAuthor = resolutionsOfInterest.execId
                                 where (resolutionsOfInterest.execDate IS NULL OR
                                        resolutionsOfInterest.execDate >  $P{DATE.End})
                                   AND LastReportT.reportId IS NOT NULL
                                   AND reportApproved IS NULL
                                   AND reportDeclined IS NULL
                                 group by execId),

     UnexecutedTotalT AS
         (SELECT resolutionsOfInterest.execId,
                 count(distinct resolutionsOfInterest.resolutId) UnexecutedTotal
          FROM resolutionsOfInterest
          WHERE resolutionsOfInterest.execDate IS NULL
             OR resolutionsOfInterest.execDate >  $P{DATE.End}
          group by execId),
/* резолюции */

/* поручения */
/* все поручения, соответствующие условиям отбора */
     tasksOfInterest AS (
         SELECT task.id                        taskId,
                resExecCurr.executorcurr       execId,
                executorsOfInterest.execName   execName,
                task.resolution                taskText,
                task.ctrldateexecution         execDate,
                CASE
                    WHEN exDateCur.execdatecurr BETWEEN '0001.01.02' AND  $P{DATE.End} THEN exDateCur.execdatecurr
                    ELSE task.ctrldeadline END AS ctrlDeadline,
                exDateCur.idx                  execIndex
         FROM f_dp_tasksresolution task
                  JOIN f_dp_rkkbase rkkBase on task.id = rkkBase.id
                  JOIN f_dp_tasksresolution_ec resExecCurr ON task.id = resExecCurr.owner
                  JOIN executorsOfInterest ON resExecCurr.executorcurr = executorsOfInterest.execId
                  JOIN f_dp_tasksresolution_edc exDateCur ON task.id = exDateCur.owner AND resExecCurr.idx = exdatecur.idx
         WHERE task.isproject = 0
           AND rkkBase.isdeleted = 0
           AND task.ctrliscontrolled = 1
           AND (
             task.ctrldateexecution BETWEEN  $P{DATE.Start} AND  $P{DATE.End}
                 OR
                 ((task.ctrldateexecution IS NULL OR task.ctrldateexecution >  $P{DATE.End})
                 AND task.ctrldeadline <  $P{DATE.End})
             )
     ),

/* количество поручений, всего */
     TasksInDocumentT AS (SELECT count(DISTINCT tasksOfInterest.taskId) TasksInDocument FROM tasksOfInterest),

/* количество поручений, по каждому исполнителю */
     totalTasksT AS (SELECT tasksOfInterest.execId,
                            tasksOfInterest.execName,
                            count(DISTINCT tasksOfInterest.taskId) totalTasks
                     FROM tasksOfInterest
                     group by execId,execName),

/* количество исполненных поручений, по каждому исполнителю */
     totalExecTaskT AS (SELECT tasksOfInterest.execId,
                               count(distinct tasksOfInterest.taskId) totalExecTask
                        FROM tasksOfInterest
                        WHERE execDate <=  $P{DATE.End}
                        group by execId),

/* количество поручений, исполненных с нарушением срока, по каждому исполнителю */
     ExecTaskDeadMissedT AS (SELECT tasksOfInterest.execId,
                                    count(distinct tasksOfInterest.taskId) ExecTaskDeadMissed
                             FROM tasksOfInterest
                             WHERE execDate <=  $P{DATE.End}
                                 AND ctrlDeadline < execDate
                             group by execId),

/* отчеты по неисполненным поручениям */
     LastTasksReportT AS (SELECT reportId, taskParent, reportAuthor, reportDate, reportDeclined, reportApproved
                          FROM (SELECT report.id            reportId,
                                       report.hierparent    taskParent,
                                       report.author        reportAuthor,
                                       report.execdate      reportDate,
                                       NULLIF(trim(report.declinewhopid),'') reportDeclined,
                                       report.approvedt     reportApproved,
                                       row_number() over (PARTITION BY tasksOfInterest.execId, tasksOfInterest.taskId
                                           ORDER BY report.execdate DESC, report.created_date DESC) rowNumber
                                FROM tasksOfInterest
                                         JOIN f_dp_report report
                                              ON report.hierparent = tasksOfInterest.taskId
                                                  AND report.author = tasksOfInterest.execId
                                WHERE (tasksOfInterest.execDate IS NULL OR tasksOfInterest.execDate >  $P{DATE.End})
                                  AND report.isdeleted = 0
                                  AND report.type = 1
                               ) orderedReports
                          WHERE rowNumber = 1),

/* количество неисполненных поручений, со снятым признаком Контроль исполнителя (без отчета), по каждому исполнителю */
/* отсутствие записи в f_dp_tasksresolution_excl означает снятие контроля с исполнителя */
     UnexecTasksCtrlRemovedT AS (SELECT tasksOfInterest.execId,
                                        count(distinct tasksOfInterest.taskId) UnexecTasksCtrlRemoved
                                 from tasksOfInterest
                                          LEFT JOIN f_dp_tasksresolution_excl execOut
                                                    ON execOut.owner = tasksOfInterest.taskId
                                                        AND execOut.idx = tasksOfInterest.execIndex
                                          LEFT JOIN LastTasksReportT
                                                    ON LastTasksReportT.taskParent = tasksOfInterest.taskId
                                                        AND LastTasksReportT.reportAuthor = tasksOfInterest.execId
                                 where (tasksOfInterest.execDate IS NULL OR tasksOfInterest.execDate >  $P{DATE.End})
                                   AND execOut.id is null
                                   AND (LastTasksReportT.reportApproved is null OR LastTasksReportT.reportDeclined is not null)
                                 group by execId),

/* количество неисполненных поручений, со снятым признаком Контроль исполнителя (с отчетом), по каждому исполнителю */
     UnexecTasksCtrlReportApprovedT AS (SELECT tasksOfInterest.execId,
                                               count(distinct tasksOfInterest.taskId) UnexecTasksCtrlReportApproved
                                        from tasksOfInterest
                                                 LEFT JOIN f_dp_tasksresolution_excl execOut
                                                           ON execOut.owner = tasksOfInterest.taskId
                                                               AND execOut.idx = tasksOfInterest.execIndex
                                                 LEFT JOIN LastTasksReportT
                                                           ON LastTasksReportT.taskParent = tasksOfInterest.taskId
                                                               AND LastTasksReportT.reportAuthor = tasksOfInterest.execId
                                        where (tasksOfInterest.execDate IS NULL OR tasksOfInterest.execDate >  $P{DATE.End})
                                          AND execOut.id is null
                                          AND LastTasksReportT.reportApproved IS NOT NULL
                                          AND LastTasksReportT.reportDeclined IS NULL
                                        group by execId),

/* количество неисполненных поручений, с признаком Контроль исполнителя (без отчета), по каждому исполнителю */
     UnexecTasksCtrlReportDeclinedT AS (SELECT tasksOfInterest.execId,
                                               count(distinct tasksOfInterest.taskId) UnexecTasksCtrlReportDeclined
                                        from tasksOfInterest
                                                 JOIN f_dp_tasksresolution_excl execOut
                                                      ON execOut.owner = tasksOfInterest.taskId
                                                          AND execOut.idx = tasksOfInterest.execIndex
                                                 LEFT JOIN LastTasksReportT
                                                           ON LastTasksReportT.taskParent = tasksOfInterest.taskId
                                                               AND LastTasksReportT.reportAuthor = tasksOfInterest.execId
                                        where (tasksOfInterest.execDate IS NULL OR tasksOfInterest.execDate >  $P{DATE.End})
                                          AND (LastTasksReportT.reportId IS NULL OR reportDeclined IS NOT NULL)
                                        group by execId),

/* количество неисполненных поручений, с признаком Контроль исполнителя (c отчетом), по каждому исполнителю */
     UnexecTasksCtrlReportMissedT AS (SELECT tasksOfInterest.execId,
                                             count(distinct tasksOfInterest.taskId) UnexecTasksCtrlReportMissed
                                      from tasksOfInterest
                                               JOIN LastTasksReportT
                                                    ON LastTasksReportT.taskParent = tasksOfInterest.taskId
                                                        AND LastTasksReportT.reportAuthor = tasksOfInterest.execId
                                      where (tasksOfInterest.execDate IS NULL OR tasksOfInterest.execDate >  $P{DATE.End})
                                        AND LastTasksReportT.reportApproved IS NULL
                                        AND LastTasksReportT.reportDeclined IS NULL
                                      group by execId),

     UnexecutedTasksTotalT AS (SELECT tasksOfInterest.execId,
                                      count(distinct tasksOfInterest.taskId) UnexecutedTasksTotal
                               FROM tasksOfInterest
                               WHERE tasksOfInterest.execDate IS NULL OR tasksOfInterest.execDate >  $P{DATE.End}
                               group by execId)
/* поручения */

select executorsOfInterest.execName,
       coalesce(ResolutionsInDocument, 0) + coalesce (TasksInDocument, 0)    ResolutionsInDocument,
       coalesce(totalResolutions, 0) + coalesce(totalTasks, 0)               totalResolutions,
       coalesce(totalExecRes, 0) + coalesce(totalExecTask, 0)                totalExecRes,
       coalesce(ExecResDeadMissed, 0) + coalesce(ExecTaskDeadMissed, 0)      ExecResDeadMissed,
       coalesce(UnexecCtrlRemoved, 0) +  coalesce(UnexecTasksCtrlRemoved, 0) UnexecCtrlRemoved,
       coalesce(UnexecCtrlReportApproved, 0) + coalesce(UnexecTasksCtrlReportApproved, 0)  UnexecCtrlReportApproved,
       coalesce(UnexecCtrlReportDeclined, 0) + coalesce(UnexecTasksCtrlReportDeclined, 0) UnexecCtrlReportDeclined,
       coalesce(UnexecCtrlReportMissed, 0) + coalesce(UnexecTasksCtrlReportMissed, 0) UnexecCtrlReportMissed,
       coalesce(UnexecutedTotal, 0) +  coalesce(UnexecutedTasksTotal, 0)          UnexecutedTotal

from executorsOfInterest
         LEFT JOIN ResolutionsInDocumentT ON 1=1     /* резолюции */
         LEFT JOIN TasksInDocumentT ON 1=1           /* поручения */
         LEFT JOIN totalResolutionsT ON totalResolutionsT.execId = executorsOfInterest.execId /* резолюции */
         LEFT JOIN totalTasksT ON totalTasksT.execId = executorsOfInterest.execId             /* поручения */
         LEFT JOIN totalExecResT ON totalExecResT.execId = totalResolutionsT.execId           /* резолюции */
         LEFT JOIN totalExecTaskT ON totalExecTaskT.execId = totalTasksT.execId               /* поручения */
         LEFT JOIN ExecResDeadMissedT ON ExecResDeadMissedT.execId = totalResolutionsT.execId /* резолюции */
         LEFT JOIN ExecTaskDeadMissedT ON ExecTaskDeadMissedT.execId = totalTasksT.execId     /* поручения */
         LEFT JOIN UnexecCtrlRemovedT ON UnexecCtrlRemovedT.execId = totalResolutionsT.execId /* резолюции */
         LEFT JOIN UnexecTasksCtrlRemovedT ON UnexecTasksCtrlRemovedT.execId = totalTasksT.execId /* поручения */
         LEFT JOIN UnexecCtrlReportApprovedT ON UnexecCtrlReportApprovedT.execId = totalResolutionsT.execId /* резолюции */
         LEFT JOIN UnexecTasksCtrlReportApprovedT ON UnexecTasksCtrlReportApprovedT.execId = totalTasksT.execId /* поручения */
         LEFT JOIN UnexecCtrlReportMissedT ON UnexecCtrlReportMissedT.execId = totalResolutionsT.execId /* резолюции */
         LEFT JOIN UnexecTasksCtrlReportMissedT ON UnexecTasksCtrlReportMissedT.execId = totalTasksT.execId /* поручения */
         LEFT JOIN UnexecCtrlReportDeclinedT ON UnexecCtrlReportDeclinedT.execId = totalResolutionsT.execId /* резолюции */
         LEFT JOIN UnexecTasksCtrlReportDeclinedT ON UnexecTasksCtrlReportDeclinedT.execId = totalTasksT.execId /* поручения */
         LEFT JOIN UnexecutedTotalT ON UnexecutedTotalT.execId = totalResolutionsT.execId /* резолюции */
         LEFT JOIN UnexecutedTasksTotalT ON UnexecutedTasksTotalT.execId = totalTasksT.execId /* поручения */
order by execName]]>
	</queryString>
	<field name="execName" class="java.lang.String"/>
	<field name="ResolutionsInDocument" class="java.lang.Integer"/>
	<field name="totalResolutions" class="java.lang.Integer"/>
	<field name="totalExecRes" class="java.lang.Integer"/>
	<field name="ExecResDeadMissed" class="java.lang.Integer"/>
	<field name="UnexecCtrlRemoved" class="java.lang.Integer"/>
	<field name="UnexecCtrlReportApproved" class="java.lang.Integer"/>
	<field name="UnexecCtrlReportDeclined" class="java.lang.Integer"/>
	<field name="UnexecCtrlReportMissed" class="java.lang.Integer"/>
	<field name="UnexecutedTotal" class="java.lang.Integer"/>
	<variable name="totalResolutionsSum" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$F{totalResolutions}]]></variableExpression>
		<initialValueExpression><![CDATA[0L]]></initialValueExpression>
	</variable>
	<variable name="totalExecResSum" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$F{totalExecRes}]]></variableExpression>
		<initialValueExpression><![CDATA[0L]]></initialValueExpression>
	</variable>
	<variable name="ExecResDeadMissedSum" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$F{ExecResDeadMissed}]]></variableExpression>
		<initialValueExpression><![CDATA[0L]]></initialValueExpression>
	</variable>
	<variable name="UnexecCtrlRemovedSum" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$F{UnexecCtrlRemoved}]]></variableExpression>
		<initialValueExpression><![CDATA[0L]]></initialValueExpression>
	</variable>
	<variable name="UnexecCtrlReportApprovedSum" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$F{UnexecCtrlReportApproved}]]></variableExpression>
		<initialValueExpression><![CDATA[0L]]></initialValueExpression>
	</variable>
	<variable name="UnexecCtrlReportDeclinedSum" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$F{UnexecCtrlReportDeclined}]]></variableExpression>
		<initialValueExpression><![CDATA[0L]]></initialValueExpression>
	</variable>
	<variable name="UnexecCtrlReportMissedSum" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$F{UnexecCtrlReportMissed}]]></variableExpression>
		<initialValueExpression><![CDATA[0L]]></initialValueExpression>
	</variable>
	<variable name="UnexecutedTotalSum" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$F{UnexecutedTotal}]]></variableExpression>
		<initialValueExpression><![CDATA[0L]]></initialValueExpression>
	</variable>
