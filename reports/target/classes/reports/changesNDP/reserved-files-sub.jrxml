	<parameter name="ndpChangeId" class="java.lang.Object"/>
	<queryString>
		<![CDATA[select
	ex.indexprefix,
    ex.indexnum,
    ex.indexsuffix,
	f.titlefile,
	f.storageperiod,
	(f.StoragePeriod || ', ' || f.SpDescription) as file_sp,
	(sp."period" || ', ' || sp.normdocshortname || ' ' || sp.articlenum) as spd_sp
from
	FR_UnitRegChange_reserve ur
   join FR_File f on ur.RefRegisterReserve = f.id
   join FR_File_extended ex on f.id = ex.parent
   join FR_UnitRegisterChange uc on ur.owner = uc.id
   left join spd_period sp on f.shelflife = sp.id 
where uc.id = $P{ndpChangeId}]]>
	</queryString>
