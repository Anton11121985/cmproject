<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="ISSContextualSearchColumn" id="ISSContextualSearchColumnForm" name="${ui.form.ISSContextualSearchColumnForm.name:ISSContextualSearchColumnForm}" projection="default">
    <properties>
      <hide condition-union="AND">
        <negative-condition-ref ref="colNameEqualsISSContextualSearchResTunable"/>
      </hide>
    </properties>
    <appearance field-layout="vertical"/>
    <component>
      <field attribute-ref="hyperLinkCustomized">
        <appearance importance="normal"/>
      </field>
      <field attribute-ref="snippet">
        <appearance importance="normal" widget="html"/>
      </field>
      <section id="row1" group="true">
        <appearance field-layout="horizontal"/>
        <component>
          <field attribute-ref="enactString">
            <appearance importance="normal"/>
          </field>
          <field attribute-ref="validityPeriodISS">
            <appearance importance="normal"/>
          </field>
          <field attribute-ref="notFoundWords">
            <appearance importance="normal"/>
          </field>
        </component>
      </section>
    </component>
  </form>
</ui>