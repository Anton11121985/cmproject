<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="ExampleComlexClassifier" id="ExampleComplexClassifierForm" projection="default">
    <properties>
      <hide condition-union="AND">
        <negative-condition-ref ref="edit"/>
        <negative-condition-ref ref="read"/>
      </hide>
    </properties>
    <component>
      <field attribute-ref="cityLocal" name="${ui.form.ExampleComplexClassifierForm.field.cityLocal.name:Город}">
        <appearance importance="normal"/>
      </field>
      <field attribute-ref="countryLocal" name="${ui.form.ExampleComplexClassifierForm.field.countryLocal.name:Страна}">
        <appearance importance="normal"/>
      </field>
      <field attribute-ref="streetLocal">
        <appearance importance="normal"/>
        <hint-pop-up>${ui.form.ExampleComplexClassifierForm.field.streetLocal.hintPopUp:Улица}</hint-pop-up>
      </field>
    </component>
  </form>
</ui>