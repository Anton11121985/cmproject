<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="AccountUnit" id="AccountUnitForm_Dialog" projection="default">
    <properties>
      <hide condition-union="AND">
        <negative-condition-ref ref="dialog"/>
      </hide>
    </properties>
    <component>
      <field attribute-ref="type" name="${ui.form.AccountUnitForm_Dialog.field.type.name:Вид мессенджера}">
        <appearance importance="normal"/>
        <directories limit-livesearch="sys-current">
          <directory-ref ref="Account"/>
        </directories>
      </field>
      <field attribute-ref="account" name="${ui.form.AccountUnitForm_Dialog.field.account.name:аккаунт}">
        <appearance importance="normal"/>
      </field>
    </component>
  </form>
</ui>