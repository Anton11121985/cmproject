<?xml version='1.0' encoding='UTF-8'?>
<ui xmlns="http://www.intertrust.ru/schema/palette/tn-ui">
  <form schema-ref="AcceptBusinessRegPlace" id="AcceptBusinessRegPlaceForm_Dialog" name="${ui.form.AcceptBusinessRegPlaceForm_Dialog.name:Передача дел}" projection="default">
    <properties>
      <hide condition-union="AND">
        <negative-condition-ref ref="dialog"/>
      </hide>
    </properties>
    <appearance field-layout="vertical"/>
    <component>
      <field attribute-ref="delegateUnit">
        <appearance importance="normal" widget="grid"/>
        <directories limit-livesearch="sys-current">
          <directory-ref ref="arm-admin-delegate-regplace"/>
        </directories>
      </field>
      <button name="${ui.form.AcceptBusinessRegPlaceForm_Dialog.button.Да.name:Да}">
        <action>
          <system name="submit"/>
        </action>
      </button>
      <button name="${ui.form.AcceptBusinessRegPlaceForm_Dialog.button.Отмена.name:Отмена}">
        <action>
          <system name="cancel"/>
        </action>
      </button>
    </component>
  </form>
</ui>