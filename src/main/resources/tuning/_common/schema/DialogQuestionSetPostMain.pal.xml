<?xml version='1.0' encoding='UTF-8'?>
<schemas xmlns="http://www.intertrust.ru/schema/palette/tn-schema">
  <schema extends="DialogForSave" name="DialogQuestionSetPostMain">
    <meta>
      <category name="РМ Администратора"/>
    </meta>
    <string name="dialogId" temporal="true"/>
    <boolean name="isOkCancelDialog" temporal="true"/>
    <boolean name="submitNo" createlog="true" temporal="true"/>
    <boolean name="submitYes" createlog="true" temporal="true"/>
    <string name="postName" temporal="true"/>
    <string name="messageText" temporal="true">
      <calc-values>
        <script event="open" lang="SPEL"><![CDATA['Указанная персона уже занимает основную штатную единицу: '+ postName+'.  Продолжить сохранение и сделать текущую штатную единицу основной?']]></script>
      </calc-values>
    </string>
    <boolean name="contextIsCollection" temporal="true"/>
    <string name="empunid" temporal="true"/>
    <string name="personid" temporal="true"/>
    <boolean name="submitvalue" temporal="true"/>
    <enum-ref ref="SystemGroupModuleTypesEnum" array="true" name="toCopyFrom_InternalDocs" temporal="true"/>
    <enum-ref ref="SystemGroupModuleTypesEnum" array="true" name="toCopyFrom_OutputDocs" temporal="true"/>
    <enum-ref ref="SystemGroupModuleTypesEnum" array="true" name="toCopyFrom_InputDocs" temporal="true"/>
    <enum-ref ref="SystemGroupModuleTypesEnum" array="true" name="toCopyFrom_Requests" temporal="true"/>
    <enum-ref ref="SystemGroupModuleTypesEnum" array="true" name="toCopyFrom_ContractsLite" temporal="true"/>
    <enum-ref ref="SystemGroupModuleTypesEnum" array="true" name="toCopyFrom_Missions" temporal="true"/>
    <string name="counter" temporal="true"/>
    <string name="data" createlog="true" temporal="true"/>
  </schema>
</schemas>