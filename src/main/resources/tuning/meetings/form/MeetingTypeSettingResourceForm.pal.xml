                  <directory-ref ref="CURRENT_SYS_HUMAN"/>
                  <directory-ref ref="CURRENT_SYS_HUMAN_Hierarchy"/>
                  <directory-ref ref="ALIEN_BEARDS_HUMANONLY"/>
                  <directory-ref ref="FV_CURR_HUMAN_HH_FOREIGN_HUMAN"/>
