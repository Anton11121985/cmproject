      <hide condition-union="AND">
        <negative-condition-ref ref="dialog"/>
        <negative-condition-ref ref="read"/>
      </hide>
    </properties>
    <component>
      <field attribute-ref="packageNumber" name="${ui.form.DocsListForRegistryTable.field.packageNumber.name:Номер пакета/авианакладной:}"/>
