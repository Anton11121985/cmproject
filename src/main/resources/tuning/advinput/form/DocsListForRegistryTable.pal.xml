      <field attribute-ref="from" name="${ui.form.DocsListForRegistryTable.field.from.name:Наименование отправителя:}"/>
      <field attribute-ref="to" name="${ui.form.DocsListForRegistryTable.field.to.name:Кому адресован/передан:}"/>
      <field attribute-ref="docPages" name="${ui.form.DocsListForRegistryTable.field.docPages.name:Количество листов документа:}"/>
      <field attribute-ref="appNumber" name="${ui.form.DocsListForRegistryTable.field.appNumber.name:Количество листов приложения:}"/>
    </component>
  </form>
</ui>