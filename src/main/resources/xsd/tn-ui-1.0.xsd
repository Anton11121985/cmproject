                <xs:element name="field-appearance" type="fieldAppearanceObjectType" maxOccurs="unbounded"/>
                <xs:element name="condition" type="condtionObjectType" maxOccurs="unbounded"/>
                <xs:element name="form" type="formObjectType" maxOccurs="unbounded"/>
                <xs:element name="handler" type="handlerObjectType" maxOccurs="unbounded"/>
                <xs:element name="async-handler" type="asyncHandlerObjectType" maxOccurs="unbounded"/>
                <xs:element name="projection" type="projectionObjectType" maxOccurs="unbounded"/>
                <xs:element name="roles" type="rolesType" maxOccurs="unbounded"/>
                <xs:element name="directory-modifier" type="directoryModifierNameType" maxOccurs="unbounded"/>
                <xs:element name="directories" type="directoriesType" maxOccurs="unbounded"/>
                <xs:element name="dependent-field" type="dependentFieldObjectType" maxOccurs="unbounded"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>

</xs:schema>