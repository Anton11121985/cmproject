    <xs:complexType name="paramEnumType">
        <xs:annotation>
            <xs:documentation><![CDATA[Перечисление]]></xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="value" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ref" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[Ссылка на свойство схемы типа Enum]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="value" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[Значение]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="paramSpecificationType">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[Конкретизация Схемы позволяет настроить отображение объекта по различным Формам в зависимости от определенных Свойств схемы. Результатом конкретизации является изменение Схемы данных объекта, а по измененной Схеме будет выбрана новая Форма для его отображения.]]></xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="string-ref" type="paramStringType"/>
                <xs:element name="intger-ref" type="paramIntegerType"/>
                <xs:element name="double-ref" type="paramDoubleType"/>
                <xs:element name="time-ref" type="paramTimeType"/>
                <xs:element name="date-ref" type="paramDateType"/>
                <xs:element name="datetime-ref" type="paramDateTimeType"/>
                <xs:element name="boolean-ref" type="paramBooleanType"/>
                <xs:element name="enum-ref" type="paramEnumType"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>

    <!-- Настраиваемый обработчик событий -->
    <xs:complexType name="customEventHandlerType">
        <xs:annotation>
            <xs:documentation><![CDATA[Настраиваемый обработчик событий]]></xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="lang" type="xs:string" use="optional" default="JavaScript">
                    <xs:annotation>
                        <xs:documentation><![CDATA[Язык программирования]]></xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <!-- WorkFlow -->
    <xs:complexType name="workflowType">
        <xs:annotation>
            <xs:documentation><![CDATA[WorkFlow]]></xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="source-notice">
                    <xs:annotation>
                        <xs:documentation><![CDATA[Источник уведомления данных]]></xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="xs:string">
                                <xs:attribute name="attribute-ref" type="xs:string" use="required">
                                    <xs:annotation>
                                        <xs:documentation>
                                            <![CDATA[Ссылка на св-во схемы с признаком wf-link]]></xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="param">
                    <xs:annotation>
                        <xs:documentation><![CDATA[Параметры выражений]]></xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="xs:string">
                                <xs:attribute name="name" use="optional">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                            <xs:enumeration value="root-unid">
                                                <xs:annotation>
                                                    <xs:documentation>
                                                        <![CDATA[Идентификатор сущности самого верхнего уровня иерархии]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="req-type">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Вид документа]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="subject">
                                                <xs:annotation>
                                                    <xs:documentation>
                                                        <![CDATA[Текст для отображения в очереди уведомлений]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="created">
                                                <xs:annotation>
                                                    <xs:documentation>
                                                        <![CDATA[Дата-время создания документа]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="created-user">
                                                <xs:annotation>
                                                    <xs:documentation>
                                                        <![CDATA[Персона, создавшая документ]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="is-on-control">
                                                <xs:annotation>
                                                    <xs:documentation>
                                                        <![CDATA[Признак "На контроле"]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="has-attachments">
                                                <xs:annotation>
                                                    <xs:documentation>
                                                        <![CDATA[Признак наличия содержания]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="has-linked-documents">
                                                <xs:annotation>
                                                    <xs:documentation>
                                                        <![CDATA[Признак наличия связанных документов]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="signer">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Подписант]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="correspondent">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Корреспондент]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="reg-status">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Статус регистрации]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="sign-status">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Статус подписи]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="themes">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Список тематик]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="reg-date">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Дата регистрации]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="reg-number">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Номер регистрации]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="full-number">
                                                <xs:annotation>
                                                    <xs:documentation>
                                                        <![CDATA[Номер сущности или полный регистрационный номер сущности]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="reg-place">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Место регистрации]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="control-date">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Срок контроля]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="exec-date">
                                                <xs:annotation>
                                                    <xs:documentation>
                                                        <![CDATA[Дата-время исполнения]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="deadline">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Срок исполнения]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="is-project">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Признак "Проект"]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="has-high-priority">
                                                <xs:annotation>
                                                    <xs:documentation><![CDATA[Признак "Приоритет"]]></xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:attribute>
                                <xs:attribute name="lang" type="xs:string" use="optional" default="SPEL">
                                    <xs:annotation>
                                        <xs:documentation><![CDATA[Язык программирования]]></xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="schemaType">
        <xs:annotation>
            <xs:documentation><![CDATA[Схема]]></xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="meta" type="metaType" minOccurs="0"/>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="string" type="stringType"/>
                <xs:element name="datetime" type="datetimeType"/>
                <xs:element name="date" type="dateType"/>
                <xs:element name="daterange" type="dateRangeType"/>
                <xs:element name="time" type="timeType"/>
                <xs:element name="integer" type="integerType"/>
                <xs:element name="double" type="doubleType"/>
                <xs:element name="boolean" type="booleanType"/>
                <xs:element name="file" type="fileType"/>
                <xs:element name="richtext" type="richtextType"/>
                <xs:element name="hyperlink" type="hyperlinkType"/>
                <xs:element name="email" type="emailType"/>
                <xs:element name="actor" type="actorType"/>
                <xs:element name="enum-ref" type="enumRefType"/>
                <xs:element name="schema-ref" type="schemaRefType"/>
                <xs:element name="enum" type="enumType"/>
                <xs:element name="schema" type="schemaType"/>
                <xs:element name="param-specification" type="paramSpecificationType" minOccurs="0"/>
                <xs:element name="custom-event-handler" type="customEventHandlerType" minOccurs="0"/>
                <xs:element name="workflow" type="workflowType" minOccurs="0"/>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="name" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[Название схемы]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="workflow" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation><![CDATA[Используется WorkFlow]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="recalc" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation><![CDATA[Признак обновления свойств схемы]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="extends" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation><![CDATA[Название родительской схемы]]></xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:element name="schemas">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element name="enum" type="enumType" maxOccurs="unbounded" minOccurs="0"/>
                <xs:element name="schema" type="schemaType" maxOccurs="unbounded" minOccurs="0"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>

</xs:schema>